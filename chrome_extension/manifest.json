{
  "manifest_version": 3,
  "name": "Chrome YT Summarizer",
  "version": "1.0",
  "permissions": ["activeTab"],
  "host_permissions": [
    "http://192.168.99.50:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "YT Summarize"
  }
}